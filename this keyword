class Demo {
    int x;

    Demo(int x) {
        this.x = x;  // referring to instance variable
    }

    void show() {
        System.out.println("Value = " + x);
    }
}

public class ThisKeywordDemo {
    public static void main(String[] args) {
        Demo d = new Demo(50);
        d.show();
    }
}
