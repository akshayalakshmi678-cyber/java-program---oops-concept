class Student {
    String name;
    int age;

    Student(String n, int a) {
        name = n;
        age = a;
    }

    void show() {
        System.out.println(name + " " + age);
    }
}

public class ConstructorDemo {
    public static void main(String[] args) {
        Student s = new Student("John", 21);
        s.show();
    }
}
